<?xml version="1.0"?>
<rdf:RDF xmlns="urn:webprotege:ontology:a14c99eb-ce38-4d2a-832d-778b17bacca0#"
     xml:base="urn:webprotege:ontology:a14c99eb-ce38-4d2a-832d-778b17bacca0"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:webprotege="http://webprotege.stanford.edu/">
    <owl:Ontology rdf:about="urn:webprotege:ontology:a14c99eb-ce38-4d2a-832d-778b17bacca0"/>

    <!-- Public Health Emergency Class -->
    <owl:Class rdf:about="http://webprotege.stanford.edu/PublicHealthEmergency">
        <rdfs:label xml:lang="zh-cn">公共卫生紧急事件</rdfs:label>
    </owl:Class>

    <!-- Covid-19 Class -->
    <owl:Class rdf:about="http://webprotege.stanford.edu/Covid19">
        <rdfs:label xml:lang="zh-cn">新冠</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/PublicHealthEmergency"/>
    </owl:Class>

    <!-- H1N1 Class -->
    <owl:Class rdf:about="http://webprotege.stanford.edu/H1N1">
        <rdfs:label xml:lang="zh-cn">甲流</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/PublicHealthEmergency"/>
    </owl:Class>

    <!-- Avian Influenza Class -->
    <owl:Class rdf:about="http://webprotege.stanford.edu/AvianInfluenza">
        <rdfs:label xml:lang="zh-cn">禽流感</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://webprotege.stanford.edu/PublicHealthEmergency"/>
    </owl:Class>

    <!-- StartDate Property -->
    <owl:DatatypeProperty rdf:about="http://webprotege.stanford.edu/StartDate">
        <rdfs:label xml:lang="zh-cn">起始日期</rdfs:label>
        <rdfs:domain rdf:resource="http://webprotege.stanford.edu/PublicHealthEmergency"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    </owl:DatatypeProperty>


<!-- CaseCount Property -->
<owl:DatatypeProperty rdf:about="http://webprotege.stanford.edu/CaseCount">
    <rdfs:label xml:lang="zh-cn">病例数</rdfs:label>
    <rdfs:domain rdf:resource="http://webprotege.stanford.edu/PublicHealthEmergency"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<!-- DeathCount Property -->
<owl:DatatypeProperty rdf:about="http://webprotege.stanford.edu/DeathCount">
    <rdfs:label xml:lang="zh-cn">死亡数</rdfs:label>
    <rdfs:domain rdf:resource="http://webprotege.stanford.edu/PublicHealthEmergency"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<!-- Covid-19 Instance -->
<owl:NamedIndividual rdf:about="http://webprotege.stanford.edu/instance/Covid19">
    <rdf:type rdf:resource="http://webprotege.stanford.edu/Covid19"/>
    <webprotege:StartDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-12-01</webprotege:StartDate>
    <webprotege:CaseCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">100000000</webprotege:CaseCount>
    <webprotege:DeathCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2000000</webprotege:DeathCount>
</owl:NamedIndividual>

<!-- H1N1 Instance -->
<owl:NamedIndividual rdf:about="http://webprotege.stanford.edu/instance/H1N1">
    <rdf:type rdf:resource="http://webprotege.stanford.edu/H1N1"/>
    <webprotege:StartDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2009-04-01</webprotege:StartDate>
    <webprotege:CaseCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1000000</webprotege:CaseCount>
    <webprotege:DeathCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10000</webprotege:DeathCount>
</owl:NamedIndividual>

<!-- Avian Influenza Instance -->
<owl:NamedIndividual rdf:about="http://webprotege.stanford.edu/instance/AvianInfluenza">
    <rdf:type rdf:resource="http://webprotege.stanford.edu/AvianInfluenza"/>
    <webprotege:StartDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2006-01-01</webprotege:StartDate>
    <webprotege:CaseCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">50000</webprotege:CaseCount>
    <webprotege:DeathCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5000</webprotege:DeathCount>
</owl:NamedIndividual>

</rdf:RDF>
